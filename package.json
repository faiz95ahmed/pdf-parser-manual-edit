{
	"name": "pdf-parser-manual-edit",
	"displayName": "pdf-parser-manual-edit",
	"description": "",
	"version": "0.0.1",
	"publisher": "faiza-deepzen",
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.poiLoad",
		"onCommand:extension.poiCycle",
		"onCommand:extension.poiNext",
		"onCommand:extension.poiPrev"
	],
	"main": "./out/extension.js",
	"contributes": {
		"keybindings": [{
			"command": "extension.poiLoad",
			"key": "ctrl+f1",
			"mac": "cmd+f1",
			"when": "editorTextFocus"
		},
		{
			"command": "extension.poiCycle",
			"key": "ctrl+f2",
			"mac": "cmd+f2",
			"when": "editorTextFocus"
		},
		{
			"command": "extension.poiNext",
			"key": "ctrl+f3",
			"mac": "cmd+f3",
			"when": "editorTextFocus"
		},
		{
			"command": "extension.poiPrev",
			"key": "ctrl+f4",
			"mac": "cmd+f4",
			"when": "editorTextFocus"
		}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	}
}
